<?jelly escape-by-default='true'?>
<j:jelly xmlns:j="jelly:core" xmlns:f="/lib/form">
    <f:entry title="${%title.columnName}" field="columnName">
        <f:textbox default="${%Coverage}"/>
    </f:entry>

    <f:entry title="${%title.baseline}" field="baseline">
        <f:select/>
    </f:entry>

    <f:entry title="${%title.metric}" field="metric">
        <f:select/>
    </f:entry>
</j:jelly>
